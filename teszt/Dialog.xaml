<Window x:Class="RobotMoverGUI.Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Adatbekérés" Height="134.999" Width="291.878">
    <Grid Margin="0,0,-2,-1">
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Height="23" Margin="10,28,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" PreviewTextInput="IntegerValidationTextBox"
                 TextChanged="TextBox_TextChanged" />
        <Label x:Name="Label" Content="Adat" HorizontalAlignment="Left" Margin="10,2,0,0" VerticalAlignment="Top" />
        <Button x:Name="Button" Content="OK" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top"
                Width="75" Click="button_Click" />
        <Label x:Name="Label1" Content="Szélesség" HorizontalAlignment="Left" Margin="135,2,0,0"
               VerticalAlignment="Top" Visibility="Hidden" />
        <TextBox x:Name="TextBox1" HorizontalAlignment="Left" Height="23" Margin="135,28,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" Visibility="Hidden" PreviewTextInput="IntegerValidationTextBox"
                 TextChanged="TextBox_TextChanged" />
    </Grid>
</Window>